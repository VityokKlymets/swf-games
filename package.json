{
  "name": "swf-games-ssr",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build-server": "babel ./server/src --out-dir build",
    "build-client": "next build",
    "server": "nodemon -r esm ./server/src",
    "client": "next",
    "build": "npm run build-server && npm run build-client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "start": "cross-env NODE_ENV=production node build/index.js"
  },
  "nodemonConfig": {
    "ignore": [
      ".next/*",
      "pages/*",
      ".git/*"
    ],
    "delay": "2500"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VityokKlymets/swf-games.git"
  },
  "dependencies": {
    "apollo-boost": "^0.1.20",
    "apollo-cache-inmemory": "^1.3.9",
    "apollo-server-express": "^2.2.0",
    "apollo-upload-client": "^9.1.0",
    "cors": "^2.8.5",
    "dotenv": "^6.1.0",
    "esm": "^3.0.84",
    "express": "^4.16.4",
    "graphql": "^14.0.2",
    "graphql-upload": "^8.0.1",
    "jsonwebtoken": "^8.3.0",
    "mkdir-recursive": "^0.4.0",
    "mongoose": "^5.3.11",
    "next": "^7.0.2",
    "next-apollo": "^2.0.2",
    "react": "^16.7.0-alpha.2",
    "react-apollo": "^2.2.4",
    "react-dom": "^16.7.0-alpha.2",
    "react-files": "^2.4.7",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-swf": "^1.0.7",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.7",
    "redux-thunk": "^2.3.0",
    "rimraf": "^2.6.2",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.83.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "babel-loader": "^8.0.4",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "nodemon": "^1.18.6"
  },
  "keywords": [],
